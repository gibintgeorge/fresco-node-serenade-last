{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-indent-props": "off",
    "indent": "off",
    "spaced-comment": "off",
    "react/jsx-indent": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "no-tabs": 0,
    "no-nested-ternary": "off",
    "class-methods-use-this": "off",
    "react/no-did-mount-set-state": "off",
    "react/prefer-stateless-function": "off",
    "no-mixed-spaces-and-tabs": "off",
    "react/no-unused-state": "off",
    "quotes": "off",
    "semi": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "vars-on-top": "off",
    "eol-last": "off",
    "key-spacing": "off",
    "eqeqeq": "off",
    "no-shadow": "off",
    "global-require": "off",
    "object-shorthand": "off",
    "no-unused-expressions": "off",
    "curly": "off",
    "no-var": "off",
    "no-trailing-spaces": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "comma-dangle": "off",
    "space-before-function-paren": "off",
    "padded-blocks": "off",
    "no-else-return": "off",
    "no-console": "off",
    "semi-spacing": "off",
    "linebreak-style": "off"
  }
}
